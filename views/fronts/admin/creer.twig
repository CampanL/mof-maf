{% extends 'layouts/admin_master.twig' %}

{% block content %}
    <form action="/admin/administrateurs/create" method="post">
    	<input type="text" id="mail" name="mail" placeholder="mail">
    	<input type="password" id="password" name="password" placeholder="mdp">
    	<input type="submit" value="Inscrire">
    </form>
    {% for alert in app.session.flashbag.get('errors') %}
        <p>{{alert}}</p>
    {% endfor %}
{% endblock %}